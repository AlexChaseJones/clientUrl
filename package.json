{
  "name": "generator-react-express-webpack-babel",
  "version": "3.2.0",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "description": "Boilerplate for ReactJS project with ExpressJS server. It uses webpack and babel",
  "main": "server.bundle.js",
  "scripts": {
    "start": "node server.bundle.js",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tahnik/react-express-webpack-babel"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "babel",
    "yeoman-generator"
  ],
  "author": "Tahnik Mustasin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tahnik/react-express-webpack-babel/issues"
  },
  "homepage": "https://github.com/tahnik/react-express-webpack-babel#readme",
  "dependencies": {
    "autoprefixer": "^7.0.1",
    "axios": "^0.16.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.4.5",
    "enzyme": "^2.8.2",
    "express": "^4.14.0",
    "jquery": "^3.2.1",
    "memcached": "^2.2.2",
    "moment": "^2.18.1",
    "node-rest-client": "^3.1.0",
    "postcss-middleware": "^1.1.3",
    "precss": "^1.4.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.1",
    "eslint-config-rallycoding": "^3.2.0",
    "jest": "^20.0.0",
    "react-test-renderer": "^15.5.4",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.2.0"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  }
}
